{
  "name": "traktsync",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "nest start --env-file .env",
    "start:local": "npm run start -- --entryFile local",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start:dev -- --debug",
    "start:prod": "node dist/main",
    "validate": "npm run lint && npm run build && npm run test",
    "test": "npm run test:unit && npm run test:mutation",
    "test:cov": "npm run test:unit:cov",
    "test:unit": "jest --config ./test/jest-unit.js",
    "test:unit:cov": "npm run test:unit -- --coverage",
    "test:mutation": "stryker run"
  },
