    "axios": "^1.8.1",
    "discord.js": "^14.18.0",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "nest-winston": "^1.10.2",
    "nestjs-zod": "^4.3.1",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "whatsapp-web.js": "^1.26.0",
    "winston": "^3.17.0",
